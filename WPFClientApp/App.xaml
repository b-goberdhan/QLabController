<Application x:Class="WPFClientApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WPFClientApp"
             xmlns:converters="clr-namespace:WPFClientApp.Helpers.Converter"
             xmlns:ninject="clr-namespace:WPFClientApp.Ninject"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <converters:BoolToVisConverter x:Key="BoolToVisConverter"></converters:BoolToVisConverter>
        <ninject:NinjectLocator x:Key="NinjectLocator"></ninject:NinjectLocator>
    </Application.Resources>
</Application>
